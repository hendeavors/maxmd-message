{
    "name": "endeavors/maxmd-message",
    "description": "Provide an easy api to integrate with maxmd",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Rodriguez",
            "email": "adam_rodriguez@bitbucket.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "endeavors/maxmd-support": "^1.0",
        "endeavors/support-vo": "^2.0",
        "endeavors/sqlserver-moment": "1.0.*@dev",
        "ext-xsl": "*",
        "php-imap/php-imap": "3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Endeavors\\MaxMD\\Message\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\MaxMD\\Message\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "3.2.*",
        "phpunit/phpunit": "~5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
