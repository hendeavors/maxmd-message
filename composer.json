{
    "name": "endeavors/maxmd-message",
    "description": "Maxmd messaging api php client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Rodriguez",
            "email": "adam_rodriguez@bitbucket.org"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bpedroza/maxmd-support.git"
        }
    ],
    "require": {
        "endeavors/maxmd-support": "dev-developer-options",
        "endeavors/support-vo": "^2.0",
        "endeavors/sqlserver-moment": "1.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Endeavors\\MaxMD\\Message\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\MaxMD\\Message\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
